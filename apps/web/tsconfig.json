{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"],
      "@socialinbox/shared/*": ["../../packages/shared/src/*"],
      "@socialinbox/ui/*": ["../../packages/ui/src/*"],
      "@socialinbox/flow-engine/*": ["../../packages/flow-engine/src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}